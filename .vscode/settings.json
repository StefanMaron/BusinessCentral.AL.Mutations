{
  "al.enableCodeActions": true,
  "al.enableCodeAnalysis": true,
  "al.codeAnalyzers": [
    "${CodeCop}",
    "${UICop}",
    "${PerTenantExtensionCop}"
  ],
  "al.ruleSetPath": ".vscode/ruleset.json",
  "al.assemblyProbingPaths": [
    ".netpackages",
    ".alpackages"
  ],
  "al.packageCachePath": ".alpackages",
  "al.enableObjectIdBadges": true,
  "files.exclude": {
    "**/.alpackages": true,
    "**/.netpackages": true,
    "**/.output": true,
    "**/.alcache": true
  },
  "search.exclude": {
    "**/.alpackages": true,
    "**/.netpackages": true,
    "**/.output": true,
    "**/.alcache": true
  },
  "files.associations": {
    "*.al": "al"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 4,
  "editor.insertSpaces": true
}
